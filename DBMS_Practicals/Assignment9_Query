db.myCollection.find({"Marks":{$gt:94},$or:[{"Dept":"Comp"},{"Age":20}]}).pretty()
{
	"_id" : ObjectId("68cb8e46e5973c89a5d81a0b"),
	"id" : 1,
	"Name" : "Mokshada",
	"Marks" : 95,
	"Dept" : "Comp",
	"Age" : 20
}
{
	"_id" : ObjectId("68cb8eede5973c89a5d81a0c"),
	"id" : 2,
	"Name" : "Sanskruti",
	"Marks" : 95,
